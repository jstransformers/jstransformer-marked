{
  "name": "jstransformer-marked",
  "version": "1.4.0",
  "description": "Marked support for JSTransformers.",
  "keywords": [
    "jstransformer",
    "marked"
  ],
  "dependencies": {
    "marked": "^4.3.0"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "test-jstransformer": "^1.0.0",
    "xo": "^0.55.0"
  },
  "scripts": {
    "test": "test-jstransformer",
    "lint": "xo --space=2 --no-semicolon index.js"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jstransformers/jstransformer-marked.git"
  },
  "author": {
    "name": "JSTransformers Team",
    "url": "http://github.com/orgs/jstransformers/people"
  },
  "license": "MIT",
  "xo": {
    "rules": {
      "unicorn/prefer-module": 0,
      "unicorn/prefer-node-protocol": 0,
      "object-curly-spacing": 0
    }
  }
}
